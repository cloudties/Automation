            </class>
            <class name="com.automation.ui.ipe.testcases.tpc.TPCPageTest"  >
                <methods>
                    <include name="clickTPCInfo_Test"/>
                    <include name="tpcUpload_Test"/>
                    <include name="searchTPCFile_Test"/>
                    <include name="tpcGenerateDrawings_Test"/>
                </methods>
            </class>
            <class name="com.automation.ui.ipe.testcases.fds.FDSPageTest"  >
                <methods>
                    <include name="clickFDSInfo_Test"/>
                    <include name="searchFDSFile_Starter_Test"/>
                    <include name="fdsGenerate_Test"/>
                </methods>
            </class>
            <class name="com.automation.ui.ipe.testcases.hwd.HWDPageTest"  >

<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ipeautomation" verbose="5" >
    <!--
    How to run using testng from command prompt
    java -cp "C:\Software\selenium\selenium-java-2.53.1.jar;C:\Software\selenium\realReport_1.0.jar;C:\Projects\Automation\Automation\target\Automation-0.0.1-SNAPSHOT-jar-with-dependencies.jar;C:\Software\selenium\testng-6.9.10.jar;C:\Software\selenium\selenium-server-standalone-2.53.1.jar" org.testng.TestNG testng.xml -d test-output\
    <parameter name="appURL" value="http://localhost:8090/"/>
    <parameter name="browserType" value="chrome"/>
    mvn -X -DskipTests -Djenkinworkspace='c:\software\workspace\IPEUI' -f Honeywell.IPE.UIAutomation/pom.xml package
    -->
    <listeners>
        <listener class-name="org.testng.reporters.EmailableReporter2"/>
        <listener class-name="org.testng.reporters.SuiteHTMLReporter"/>
        <listener class-name="org.testng.reporters.TestHTMLReporter"/>
        <listener class-name="com.automation.ui.base.common.testnglisteners.InvokeMethodAdapter"/>
        <listener class-name="com.automation.ui.base.common.report.listener.RealTimeTestResultListener"/>
    </listeners>
    <test name="Home"  >
        <groups>
                <define name="StarterRun">
                    <include name="StarterRun"/>
                </define>
                <run>
                    <include name="StarterRun" />
                </run>
        </groups>
        <!--
        <packages>
        <package name="com.automation.ui.ipe.testcases.home"/>
        </packages>
        -->
        <classes >
            <class name="com.automation.ui.ipe.testcases.login.LoginTest" >
                <methods >
                    <include name="windowsProxyLogin_Test"/>
                </methods>
            </class>
            <class name="com.automation.ui.ipe.testcases.home.HomePageTest" >
                <methods >
                    <include name="launchHomePage_Test"/>
                </methods>
            </class>
            <class name="com.automation.ui.ipe.testcases.project.ProjectDefinitionPageTest"   >
                <methods>
                    <include name="scrollAndClickCreateNewProject_Starter_WithValues_Test"/>

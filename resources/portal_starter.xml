            </class>

            <class name="com.automation.ui.ipe.testcases.deq.DeQGeneralInfoPageTest"  >
                <methods>
                     <include name="scrollAndAddInputs_DEQGeneralInfo_Starter_WithValues_Test"/>
                </methods>
            </class>

            <class name="com.automation.ui.ipe.testcases.deq.DeQCommunicationPageTest"  >
                <methods>
                    <include name="scrollAndAddInputs_DEQCommunications_Starter_WithValues_Test"/>
